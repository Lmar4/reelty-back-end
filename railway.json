{
  "$schema": "https://railway.com/railway.schema.json",
  "deploy": {
    "numReplicas": 1,
    "startCommand": "NODE_OPTIONS='--max-old-space-size=7680' node build/server.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "runtime": "V2",
    "limitOverride": {
      "containers": {
        "cpu": 2,
        "memoryBytes": 8589934592
      }
    }
  }
}
