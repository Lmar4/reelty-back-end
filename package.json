{
  "name": "reelty_backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "pnpm prisma generate && tsc",
    "start": "NODE_OPTIONS='--max-old-space-size=7680' node build/src/server.js",
    "dev": "NODE_OPTIONS='--max-old-space-size=7680' tsx watch src/server.ts",
    "db:generate": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:migrate": "pnpm prisma migrate dev",
    "db:seed": "pnpm prisma db seed",
    "db:seed:plans": "tsx prisma/seed.ts",
    "sync-stripe": "node src/scripts/sync-stripe-plans.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:templates": "tsx src/scripts/test-template-generation.ts",
    "rerun-job": "ts-node src/scripts/rerunJob.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/client-sns": "^3.750.0",
    "@aws-sdk/lib-storage": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "@clerk/backend": "^1.24.3",
    "@clerk/express": "^1.3.50",
    "@clerk/types": "^4.47.0",
    "@runwayml/sdk": "^1.4.4",
    "@types/compression": "^1.7.5",
    "@types/cron": "^2.4.3",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.8.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cron": "^4.1.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "p-limit": "^6.2.0",
    "pg": "^8.13.3",
    "posthog-node": "^4.8.1",
    "puppeteer": "^24.3.0",
    "sharp": "^0.33.5",
    "stripe": "^17.7.0",
    "svix": "^1.60.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@prisma/client": "^6.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/aws-lambda": "^8.10.147",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/glob": "^8.1.0",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "node-fetch": "^2.7.0",
    "prisma": "^6.4.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "unused-imports"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "warn",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-unused-vars": "warn",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "puppeteer"
    ]
  }
}
